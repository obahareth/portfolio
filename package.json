{
  "name": "portfolio",
  "private": true,
  "description": "Daniel Spajic's portfolio",
  "scripts": {
    "build": "yarn test && gatsby build && cp _redirects public/",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "lint": "eslint src"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|md)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/src/setupTests.js",
      "<rootDir>/src/shimTests.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      ".cache",
      "node_modules"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "gatsby": "^2.0.31",
    "gatsby-image": "^2.0.17",
    "gatsby-plugin-eslint": "^2.0.1",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-manifest": "^2.0.6",
    "gatsby-plugin-offline": "^2.0.9",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-resolve-src": "^2.0.0-beta.1",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-sharp": "^2.0.8",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-source-filesystem": "^2.0.5",
    "gatsby-transformer-remark": "^2.1.9",
    "gatsby-transformer-sharp": "^2.1.5",
    "gatsby-transformer-yaml": "^2.1.3",
    "lodash": "^4.17.11",
    "react": "^16.4.2",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-burger-menu": "^2.5.4",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-plx": "^1.3.9",
    "react-scrollspy": "^3.3.5",
    "reactstrap": "^6.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-alias": "^1.1.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.4",
    "postcss-cssnext": "^3.1.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.6.0",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.2"
  }
}
